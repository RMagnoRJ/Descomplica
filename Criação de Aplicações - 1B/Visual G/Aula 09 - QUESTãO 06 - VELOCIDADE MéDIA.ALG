procedimento velocidade

var
aux, t: real

inicio
t <- (n1 - n2)
aux <- d / t
res <- aux

fimprocedimento

var
res, n1, n2, d: real

inicio
n1 <- 10
n2 <- 8
d <- 60

velocidade

escreva(res)

fimalgoritmo